<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="mybatis.inter.LogisticsMessageOperation">
    <select id="selectUserByID" parameterType="int" resultType="LogisticsMessage">
        select * from mdlogistics where id = #{id}  
    </select> 
    
    <select id="selectUsers" parameterType="string" resultMap="resultListUser">
        select * from mdlogistics
    </select>
</mapper>  
 